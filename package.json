{
  "name": "ape.pro",
  "version": "0.0.1",
  "description": "JavaScript API client for ape.pro.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "keywords": [
    "crypto",
    "solana",
    "api-client",
    "ape.pro"
  ],
  "author": {
    "name": "Braden Lamb",
    "email": "braden@retroscepter.com",
    "url": "https://retroscepter.com"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier . --write"
    ],
    "**/*.{json,md,yaml,yml}": "prettier . --write"
  },
  "devDependencies": {
    "@swc/core": "1.10.1",
    "@types/eslint": "9.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.10.2",
    "@types/qs": "6.9.17",
    "changesets": "1.0.2",
    "date-fns": "4.1.0",
    "eslint": "9.16.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0",
    "undici-types": "7.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "axios": "1.7.9",
    "eventemitter3": "5.0.1",
    "qs": "6.13.1",
    "zod": "3.24.1"
  }
}
